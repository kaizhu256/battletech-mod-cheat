     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Roger. Commander, move up to the location Meyer indicated, and we'll make a plan from there.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
