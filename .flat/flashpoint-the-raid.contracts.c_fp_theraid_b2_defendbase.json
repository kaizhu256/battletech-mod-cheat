   "dialogueContent": [
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Commander, if we lose another building we lose the payoff. You have to hold on.",
