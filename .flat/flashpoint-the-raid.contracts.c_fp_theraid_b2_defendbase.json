     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Warning commander, sensors are reading multiple contacts approaching from multiple vectors. Looks like the attacking force has brought reinforcements. ",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
