   "dialogueContent": [
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_LocalsDefault",
     "words": "Mercenary Commander, reinforcements are incoming, they are about halfway there. Maintain position and you will be relieved and reinforced shortly.",
