     "cameraDistance": "Far",
     "cameraFocusGuid": "02048097-1c04-45e5-bbe5-be481a8dc864",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Commander, I've been informed that the warehouse has some additional defense options.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
