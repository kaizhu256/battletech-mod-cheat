 "dialogueList": [
  {
   "dialogue": {
    "EncounterObjectGuid": "73df8d9c-a274-48fd-98c9-2bd0d7860e83"
   },
   "dialogueContent": [
    {
     "audioName": "audioevent_universal_hostilesInbound",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_UW_lastChance_LadyNakanoDefault",
     "words": "Luthendorff will be forced to make her way through this pass. You can use that to catch her unawares.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "cb50e1e4-aa0c-49b2-bd2e-5339c78dafb5",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Roger that, Lady Suiko. We'll get a visual and confirm she's en route.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "76b654a6-4f2c-4a6f-86e6-d4cf868335fe",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_UW_lastChance_LadyNakanoDefault",
     "words": "I shall leave you to it, Mr. Oliveira. Let none of you forget—this woman is an honorless war profiteer. Show her no mercy!",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_SumireDefault",
     "words": "Thanks for the pep talk, Lady Suiko. We can handle it from here.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    }
   ],
   "name": "Dialogue_MissionStart",
   "overrideDialogueBucketId": ""
  },
  {
   "dialogue": {
    "EncounterObjectGuid": "4011a4c3-cba2-4d22-b2b3-3b19a3297ab9"
   },
   "dialogueContent": [
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_SumireDefault",
     "words": "Don't celebrate too soon, Commander. We're being hailed by a DropShip with House Nakano's heraldry…",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_SumireDefault",
     "words": "It's Lady Suiko's daughter, Mariko. She wants to talk.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_YangDefault",
     "words": "Of course she does.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    }
   ],
   "name": "Dialogue_MissionSuccess",
   "overrideDialogueBucketId": ""
  },
  {
