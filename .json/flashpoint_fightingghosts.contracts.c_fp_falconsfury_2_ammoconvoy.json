   "dialogueContent": [
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Commander, the Gray Death Legion convoy is right on schedule, and as predicted, we're picking up Falcon's units in the area.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "8020f476-1f81-4da9-956f-a797df693984",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Move to intercept the convoy and provide cover and support while they escape.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "1a670982-15d5-44cf-b76b-32433c27749e",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Once they're safely away, you need to make a run towards Steiner controlled territory.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    }
   ],
   "name": "Dialogue_MissionStart",
   "overrideDialogueBucketId": ""
  },
  {
   "dialogue": {
    "EncounterObjectGuid": "4011a4c3-cba2-4d22-b2b3-3b19a3297ab9"
   },
   "dialogueContent": [
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Excellent work, Commander. You have successfully reached Steiner territory.",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    }
   ],
   "name": "Dialogue_MissionSuccess",
   "overrideDialogueBucketId": ""
  },
  {
   "dialogue": {
    "EncounterObjectGuid": "d3d33d95-9ed7-4686-b9eb-954ebe51cc02"
   },
   "dialogueContent": [
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "That's the whole convoy down, Commander. Damn! I'm scrubbing the mission.",
