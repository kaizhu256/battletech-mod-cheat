   },
   "dialogueContent": [
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "d3d75b1e-ace5-43e8-b037-79a2dd2b3d4e",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Signal from the Gray Death Legion convoy leader, Commander. They're headed to the extraction point now. Keep them alive!",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
    },
    {
     "audioName": "NONE",
     "cameraDistance": "Far",
     "cameraFocusGuid": "",
     "cameraHeight": "Default",
     "emote": "Default",
     "revealRadius": -1,
     "selectedCastDefId": "castDef_DariusDefault",
     "words": "Keep in mind that the convoy will try to stay near you for protection. Continue advancing to get them to the extract point!",
     "wordsColor": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
     }
